algoritmo Exercicios_operadores04
inicio
	Inteiro a, b
    escrever ("Digite dois numero inteiros para saber o resto da divisao: ");
    Texto num_a = ler ("Digite primeiro numero: ")
    Texto num_b = lert ("Digite segundo numero: ")
    a = converte para tipo inteiro(num_a)
    b = converte para tipo inteiro(num_b)
    a %= b
    escrever (null, "O resto da divisao : " + a)
fim